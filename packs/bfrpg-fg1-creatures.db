,"items":[],"effects":[],"folder":null,"sort":0,"permission":{"default":0,"8Nfv55GaLyxiP3h3":3},"flags":{"cf":{"id":"temp_llzud0h95mf","path":"Field Guide Vol. 1 Creatures#/CF_SEP/Elemental#/CF_SEP/Elemental, Wood*","color":"#000000","name":"Elemental, Wood*","children":[],"folderPath":["temp_hx5vqfk7tfo","temp_4lgfgjp0an5"],"fontColor":"#FFFFFF","icon":null,"sorting":"a","contents":["Ud182MV5OObyIXqz","AcUja6p0ovqffuFS","QMXo3rgmTeNCa1Yd"],"version":"2.4.8"}}}
