{"_id":"x5qbSTVM1Stf36wQ","name":"Kelpie","type":"monster","img":"icons/svg/mystery-man.svg","data":{"armorClass":{"value":13,"label":"BASICFANTASYRPG.ArmorClass","abbr":"BASICFANTASYRPG.ArmorClassAbbr"},"attackBonus":{"value":1,"label":"BASICFANTASYRPG.AttackBonus","abbr":"BASICFANTASYRPG.AttackBonusAbbr"},"biography":"<p><strong>Kelpies</strong> are flesh-eating freshwater cousins of the hippocampus. They are cruel and love nothing more than to drown and devour their victims.</p>","hitPoints":{"value":2,"max":8,"label":"BASICFANTASYRPG.HitPoints","abbr":"BASICFANTASYRPG.HitPointsAbbr"},"initBonus":{"value":0,"label":"BASICFANTASYRPG.InitiativeBonus"},"move":{"value":"Swim 80","label":"BASICFANTASYRPG.Movement"},"saves":{"death":{"value":11},"wands":{"value":12},"paralysis":{"value":14},"breath":{"value":15},"spells":{"value":16}},"hitDice":{"size":"d8","number":2,"mod":0,"label":"BASICFANTASYRPG.HitDice","abbr":"BASICFANTASYRPG.HitDiceAbbr"},"morale":{"value":7,"label":"BASICFANTASYRPG.Morale"},"numberAppearing":{"value":"Wild 1","label":"BASICFANTASYRPG.NumberAppearing"},"specialAbility":{"value":1,"label":"BASICFANTASYRPG.SpecialAbility"},"treasureType":{"value":"None","label":"BASICFANTASYRPG.TreasureType"},"xp":{"value":0,"label":"BASICFANTASYRPG.ExperiencePoints","abbr":"BASICFANTASYRPG.ExperiencePointsAbbr"}},"token":{"name":"Hippocampus","img":"icons/svg/mystery-man.svg","displayName":0,"actorLink":false,"width":1,"height":1,"scale":1,"mirrorX":false,"mirrorY":false,"lockRotation":false,"rotation":0,"alpha":1,"vision":false,"dimSight":0,"brightSight":0,"sightAngle":0,"light":{"alpha":0.5,"angle":0,"bright":0,"coloration":1,"dim":0,"gradual":true,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"reverse":false},"darkness":{"min":0,"max":1}},"disposition":-1,"displayBars":0,"bar1":{"attribute":"hitPoints"},"bar2":{"attribute":null},"flags":{},"randomImg":false},"items":[{"_id":"BtNPKMnpoOWAjq29","name":"2 Hooves","